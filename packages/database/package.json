{
  "name": "@gw2me/database",
  "private": true,
  "type": "module",
  "scripts": {
    "prisma:generate": "prisma generate --no-hints",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate-dev": "prisma migrate dev --name",
    "prisma:studio": "prisma studio",
    "build": "rm -rf ./lib && tsc -b"
  },
  "exports": {
    ".": {
      "types": "./lib/generated/prisma/client.d.ts",
      "browser": "./lib/generated/prisma/browser.js",
      "default": "./lib/generated/prisma/client.js"
    },
    "./enums": {
      "types": "./lib/generated/prisma/enums.d.ts",
      "default": "./lib/generated/prisma/enums.js"
    },
    "./models": {
      "types": "./lib/generated/prisma/models.d.ts",
      "default": "./lib/generated/prisma/models.js"
    },
    "./setup": {
      "types": "./lib/setup.d.ts",
      "default": "./lib/setup.js"
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@prisma/adapter-pg": "6.17.1",
    "@prisma/client": "6.17.1"
  },
  "devDependencies": {
    "@gw2treasures/tsconfig": "0.0.1",
    "@types/node": "22.18.9",
    "prisma": "6.17.1",
    "typescript": "5.9.3"
  }
}
